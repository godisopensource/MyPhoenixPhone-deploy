{
  "version": 2,
  "installCommand": "bash -c 'cd /vercel/path0/myphoenixphone && npm install --include=dev && npm --workspace apps/backend exec prisma generate --schema ./prisma/schema.prisma'",
  "builds": [
    {
      "src": "myphoenixphone/apps/web/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "myphoenixphone/apps/backend/api/**/*.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "myphoenixphone/apps/backend/prisma/**",
          "myphoenixphone/apps/backend/node_modules/.prisma/**",
          "myphoenixphone/apps/backend/node_modules/@prisma/**",
          "myphoenixphone/node_modules/.prisma/**",
          "myphoenixphone/node_modules/@prisma/**",
          "myphoenixphone/apps/backend/node_modules/swagger-ui-dist/**",
          "myphoenixphone/node_modules/swagger-ui-dist/**"
        ],
        "runtime": "nodejs20.x",
        "memory": 1024,
        "maxDuration": 10
      }
    }
  ],
  "routes": [
    { "src": "/docs", "dest": "/myphoenixphone/apps/backend/api/index.ts" },
    { "src": "/docs/(.*)", "dest": "/myphoenixphone/apps/backend/api/index.ts" },
    { "src": "/docs-json", "dest": "/myphoenixphone/apps/backend/api/index.ts" },
    { "src": "/api-json", "dest": "/myphoenixphone/apps/backend/api/index.ts" },
    { "src": "/api", "dest": "/myphoenixphone/apps/backend/api/index.ts" },
    { "src": "/api/(.*)", "dest": "/myphoenixphone/apps/backend/api/index.ts" },
    { "src": "/(.*)", "dest": "/myphoenixphone/apps/web/$1" }
  ]
}