{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "predev": "prisma generate --schema ./prisma/schema.prisma",
    "prestart": "prisma generate --schema ./prisma/schema.prisma",
    "dev": "bash -lc 'export PORT=${PORT:-3003}; nest start --watch'",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "pretest": "prisma generate --schema ./prisma/schema.prisma",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:pact": "jest --config ./test/jest-pact.json --runInBand",
    "test:integration": "jest --config ./test/jest-integration.json --runInBand",
    "test:perf": "bash test/performance/run-all-performance-tests.sh",
    "test:perf:eligibility": "k6 run test/performance/eligibility-api.js",
    "test:perf:consent": "k6 run test/performance/consent-api.js",
    "test:perf:verification": "k6 run test/performance/verification-api.js",
    "test:perf:workers": "k6 run test/performance/workers-benchmark.js",
    "prisma:generate": "prisma generate --schema ./prisma/schema.prisma",
    "prisma:migrate:dev": "prisma migrate dev --schema ./prisma/schema.prisma",
    "prisma:migrate:deploy": "prisma migrate deploy --schema ./prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema ./prisma/schema.prisma",
    "seed:demo": "tsx ../../scripts/seed-demo-data.ts"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.2.1",
    "@prisma/client": "^5.22.0",
    "@types/pdfkit": "^0.17.3",
    "@types/qrcode": "^1.5.5",
    "axios": "^1.12.2",
    "cookie-parser": "^1.4.7",
    "pdfkit": "^0.17.2",
    "prisma": "^5.22.0",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "express-session": "^1.17.3",
    "swagger-ui-express": "^5.0.1",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@pact-foundation/pact": "^16.0.2",
    "@testcontainers/postgresql": "^11.7.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "testcontainers": "^10.28.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
